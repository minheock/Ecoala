package com.ecoala.ele.mngrMember.vo;

import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

/**
 * Class Name  : MngMemberVO
 * Author      : LeeGyuHwan
 * Created Date: 2023. 11. 22.
 * Version: 1.0
 * Purpose:   
 * Description: 회원가입 기능 연습
 */
public class MngMemberVO {

	private String memId;     /** 회원 번호 */ 
	private String userId;	  /** 회원 아이디*/ 
	private String userPw;	  /** 회원 비번*/ 
	private String userNm;	  /** 회원 이름*/ 
	private String memNo;	  /** 회원 가구원 수*/ 
	private String houseType; /** 회원 주거형태 */ 
	private String houseArea; /** 회원 주거면적 */
	private String region;    /** 회원 지역 */
	public String getMemId() {
		return memId;
	}
	// 사용할 클래스
	public MngMemberVO() {

	}
	// new MngMemberVO(memId,userId,encodePw,userNm, ); 컨트롤러에 이 부분과 개수가 맞아야함
	public MngMemberVO(String memId, String userId, String userPw, String userNm, String memNo, String houseType,
			String houseArea, String region) {
		this.memId = memId;
		this.userId = userId;
		this.userPw = userPw;
		this.userNm = userNm;
		this.memNo = memNo;
		this.houseType = houseType;
		this.houseArea = houseArea;
		this.region = region;
	}
	@Override
	public String toString() {
		return "MngMemberVO [memId=" + memId + ", userId=" + userId + ", userPw=" + userPw + ", userNm=" + userNm
				+ ", memNo=" + memNo + ", houseType=" + houseType + ", houseArea=" + houseArea + ", region=" + region
				+ "]";
	}
	public void setMemId(String memId) {
		this.memId = memId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUserPw() {
		return userPw;
	}
	public void setUserPw(String userPw) {
		this.userPw = userPw;
	}
	public String getUserNm() {
		return userNm;
	}
	public void setUserNm(String userNm) {
		this.userNm = userNm;
	}
	public String getMemNo() {
		return memNo;
	}
	public void setMemNo(String memNo) {
		this.memNo = memNo;
	}
	public String getHouseType() {
		return houseType;
	}
	public void setHouseType(String houseType) {
		this.houseType = houseType;
	}
	public String getHouseArea() {
		return houseArea;
	}
	public void setHouseArea(String houseArea) {
		this.houseArea = houseArea;
	}
	public String getRegion() {
		return region;
	}
	public void setRegion(String region) {
		this.region = region;
	}
	
	

}
